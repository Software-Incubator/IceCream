    <body>

    <!-- ================================================================================================== Loader -->
        
        <div id="loader"><img class="loader-img" src="{% static 'images/loader.svg' %}"></div>
        
        
        <!-- ================================================================================================== Wrapper -->
        <!-- Event not active modal -->
        
        <!-- This modal will be shown on requesting through url for registration page and no event is active -->
        {% if messages %}
        <div class="modal fade show" id="no_event_modal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="display:block; z-index: 100000;" aria-hidden="true">
            <div class="success-modal modal-dialog" role="document">
                <!--Content-->
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="container">
                            <!-- Card deck -->
                            <div class="text-center row">
                                <h4 class="text-center  w-100">
                                    <img style="width:100%;" src="{% static 'images/noeve.svg' %}" alt="">
                                    {% for message in messages %}
                                    <span style="color: #0094ff; font-weight: 700;">{{message}}</span>
                                    {% endfor %}
                                </h4>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/.Content-->
            </div>
        </div>
        {% endif %}
        
        <!-- This modal will be shown on register button and no event is active -->
        <div class="modal fade show" id="no_event_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="display:none; z-index: 100000;" aria-hidden="true">
            <div class="success-modal modal-dialog" role="document">
                <!--Content-->
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="container">
                            <!-- Card deck -->
                            <div class="text-center row">
                                <h4 class="text-center  w-100">
                                    <img style="width:100%;" src="{% static 'images/noeve.svg' %}" alt="">
                                    <span style="color: #0094ff; font-weight: 700;">No event active at present</span>
                                </h4>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/.Content-->
            </div>
        </div>
        
        
