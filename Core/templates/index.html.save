                                            /***  We belive in animation js  **/
                                            (function($){ 
                                                $.fn.extend({  
                                                    airport: function(array) {
                                                        
                                                        var self = $(this);
                                                        var chars = ['a','b','c','d','e','f','g',' ','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z','-'];
                                                        var longest = 0;
                                                        var items = items2 = array.length;
                                                        
                                                        function pad(a,b) { return a + new Array(b - a.length + 1).join(' '); }
                                                        
                                                        $(this).empty();
                                                        
                                                        while(items--)
                                                        if(array[items].length > longest) longest = array[items].length;
                                                        
                                                        while(items2--)
                                                        array[items2] = pad(array[items2],longest);
                                                        
                                                        spans = longest;
                                                        while(spans--)
                                                        $(this).prepend("<span class='c" + spans + "'></span>");
                                                        
                                                        
                                                        function testChar(a,b,c,d){
                                                            if(c >= array.length)
                                                            setTimeout(function() { testChar(0,0,0,0); }, 1000);				
                                                            else if(d >= longest)
                                                            setTimeout(function() { testChar(0,0,c+1,0); }, 1000);
                                                            else {
                                                                $(self).find('.c'+a).html((chars[b]==" ")?"&nbsp;":chars[b]);
                                                                setTimeout(function() {
                                                                    if(b > chars.length)
                                                                    testChar(a+1,0,c,d+1);
                                                                    else if(chars[b] != array[c].substring(d,d+1).toLowerCase())
                                                                    testChar(a,b+1,c,d);
                                                                    else
                                                                    testChar(a+1,0,c,d+1);
                                                                }, 20);
                                                            }
                                                        }
                                                        
                                                        testChar(0,0,0,0);
                                                    } 
                                                }); 
                                            })(jQuery);
                                            $(".example").airport([ "Redefining Limitations", "Innovating Insanely","Learning Technologies","Sharing Intelligence" ]);
                                        </script> 
                                        
                                        <script>
                                            function validateForm() {
                                                document.getElementById('name_error').innerHTML = "";
                                                document.getElementById('email_error').innerHTML = "";
                                                document.getElementById('subject_error').innerHTML = "";
                                                document.getElementById('contact_error').innerHTML = "";
                                                document.getElementById('message_error').innerHTML = "";
                                                document.getElementById('captcha_error').innerHTML = "";
                                                
                                                var name =  document.getElementById('name').value;
                                                if (name == "") {
                                                    document.getElementById('name_error').innerHTML = "Cannot be empty";
                                                    return false;
                                                }
                                                var email =  document.getElementById('email').value;
                                                if (email == "") {
                                                    document.getElementById('email_error').innerHTML = "Cannot be empty";
                                                    return false;
                                                } else {
                                                    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                                                    if(!re.test(email)){
                                                        document.getElementById('email_error').innerHTML = "Invalid";
                                                        return false;
                                                    }
                                                }
                                                var subject =  document.getElementById('subject').value;
                                                if (subject == "") {
                                                    document.getElementById('subject_error').innerHTML = "Cannot be empty";
                                                    return false;
                                                }
                                                var contact =  document.getElementById('contact').value;
                                                if (contact == "") {
                                                    document.getElementById('contact_error').innerHTML = "Cannot be empty";
                                                    return false;
                                                } 
                                                else {
                                                    var re = /^(?:(?:\+|0{0,2})91(\s*[\ -]\s*)?|[0]?)?[6789]\d{9}|(\d[ -]?){10}\d$/;
                                                    if(!re.test(contact)){
                                                        document.getElementById('contact_error').innerHTML = "Invalid";
                                                        return false;
                                                    }
                                                }
                                                var message =  document.getElementById('message').value;
                                                if (message == "") {
                                                    document.getElementById('message_error').innerHTML = "Cannot be empty";
                                                    return false;
                                                }
                                                var v = grecaptcha.getResponse();
                                                if(v.length == 0)
                                                {
                                                    document.getElementById('captcha_error').innerHTML="Cannot be empty";
                                                    return false;
                                                }
                                                document.getElementById('status').innerHTML = "Sending...";
                                                var email;
                                                var name;
                                                var subject;
                                                var contact;
                                                var message;
                                                name = $('#name').val();
                                                email = $('#email').val();
                                                subject = $('#subject').val();
                                                contact = $('#contact').val();
                                                message = $('#message').val();
                                                $.ajax({
                                                    url: '{% url 'save_contact' %}',
                                                    data: {
                                                        'email': email,
                                                        'name': name,
                                                        'contact': contact,
                                                        'message': message,
                                                        'subject':subject,
                                                    },
                                                    dataType: 'json',
                                                    success: function (data) {
                                                        if(data['done'] == 1){
                                                            document.getElementById('status').innerHTML = data['message'];
                                                            document.getElementById('status').style.color = "green";
                                                            document.getElementById('status').classList = "text-center";
                                                            setTimeout(function() {
                                                                document.getElementById('centralModalLg2').style.display = "none";
                                                                document.querySelector('.modal-backdrop').style.display = "none";
                                                            }, 2000);
                                                        }
                                                        else if(data['done'] == 0){
                                                            document.getElementById('status').innerHTML = data['message'];
                                                            document.getElementById('status').style.color = "red";
                                                            document.getElementById('status').classList = "text-center";
                                                        }
                                                    }
                                                });
                                            }
                                            
                                            function redirect()
                                            {
                                                {% if event %}
                                                var href = event.currentTarget.getAttribute('href');
                                                window.location=href;
                                                {% else %}
                                                document.getElementById("no_event_modal").style.display = "block";
                                                setTimeout(function() {
                                                    document.getElementById('no_event_modal').style.display = "none";
                                                }, 2500);
                                                {% endif %}
                                            }
                                            
                                            window.onload = function() 
                                            {
                                                {% if messages %}
                                                setTimeout(function() {
                                                    document.getElementById('no_event_modal1').style.display = "none";
                                                }, 2500);
                                                {% endif %}
                                            }
                                        </script>
